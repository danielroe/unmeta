{
  "name": "unmeta",
  "version": "0.0.2",
  "description": "Simple tools for interacting with HTML meta tags",
  "repository": "unjs/unmeta",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build",
    "lint": "yarn lint:all:eslint && yarn lint:all:prettier",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts,.vue .",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,ts,vue}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "yarn husky install",
    "prepublishOnly": "yarn lint && yarn test && yarn pinst --disable",
    "release": "release-it",
    "test": "jest",
    "watch": "yarn build --watch",
    "postinstall": "husky install",
    "postpublish": "yarn pinst --enable"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@release-it/conventional-changelog": "^2.0.1",
    "@types/babel__core": "^7",
    "@types/escape-html": "^1",
    "@types/jest": "^26.0.24",
    "@types/mkdirp": "^1.0.2",
    "@types/mri": "^1.1.1",
    "@types/node": "^15.9.0",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "pinst": "^2.1.6",
    "prettier": "^2.7.1",
    "release-it": "^14.14.3",
    "semver": "^7.3.8",
    "siroc": "^0.16.0",
    "typescript": "^4.9.3"
  }
}
